  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --ignore-path .gitignore --write .",
    "lint": " eslint --no-error-on-unmatched-pattern --ext .ts,.tsx --ignore-path .gitignore src scripts",
    "create-cmd": "dotenv-extended src/scripts/create-command.sh",
    "get-cmds": "dotenv-extended ts-node src/scripts/get-commands.ts",
    "poll": "dotenv-extended ts-node src/scripts/poll.ts",
    "gauth": "dotenv-extended ts-node src/scripts/google-auth.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,mdx,css}": "yarn prettier",
    "*.{js,jsx,tsx,ts}": [
      "eslint --fix",
      "yarn prettier"
    ]
  },
  "dependencies": {
    "date-fns": "^3.0.0",
    "discord.js": "^14.16.3",
    "fathom-client": "^3.0.0",
    "form-data": "^4.0.0",
    "googleapis": "^144.0.0",
    "isomorphic-fetch": "^3.0.0",
    "micro-cors": "^0.1.1",
    "mongodb": "^6.3.0",
    "next": "^14.1.3",
    "next-seo": "^4.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.23.5",
    "@types/node": "^16",
    "@types/react": "^18.2.64",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.2.6",
    "dotenv-extended": "^2.9.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.35",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.0.0",
    "typescript": "^5.4.2"
  }
